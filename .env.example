# Server
PORT=4000
NODE_ENV=development
LOG_LEVEL=debug

# LLM Providers (configure at least one)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=
MISTRAL_API_KEY=
GROK_API_KEY=
DEEPSEEK_API_KEY=
LAMBDA_AI=

# OmniParser — configure at least one provider
# Priority 1: Hugging Face Gradio (Microsoft's official deployment, no cold starts)
HUGGINGFACE_OMNIPARSER_ENDPOINT=

# Priority 2: Modal.com serverless GPU
MODAL_API_KEY=
MODAL_OMNIPARSER_ENDPOINT=

# Priority 3: Replicate (fallback, has cold starts)
REPLICATE_API_TOKEN=

# OmniParser warmup — keeps Replicate model hot (only needed for Replicate)
OMNIPARSER_WARMUP_ENABLED=true

# Redis — optional, enables OmniParser result caching
REDIS_HOST=
REDIS_PORT=6379
